/dts-v1/;

#include "../../qcom/msm8992.dtsi"
#include "msm8992-p1_cn.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. MSM8992 P1 CN";
	compatible = "qcom,msm8992-cdp", "qcom,msm8992", "qcom,cdp";
	qcom,board-id = <0x564 0>;
